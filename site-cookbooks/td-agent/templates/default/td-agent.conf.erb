<source>
  type monitor_agent
  id monitor_agent_input
  bind 0.0.0.0
  port 24220
</source>

<source>
  type forward
  id source_forward
</source>

<match knock.slack>
  type slack
  id knock_slack
  webhook_url <%= @knock_url %>
  channel a-know-home-notify
  username VisitorNotification
  icon_emoji :wind_chime:
  flush_interval 10s
</match>

@include /etc/td-agent/conf.d/*.conf
